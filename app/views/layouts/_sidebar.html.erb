<div class="left-sidebar panel-group welly" id="accordion">
   <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
  <% all_themes.each do |theme| %>
  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="headingOne">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion" href="#collapse<%= theme.id %>" aria-expanded="true" aria-controls="collapseOne">
          <%= theme.name %>
        </a>
      </h4>
    </div>
    <div id="collapse<%= theme.id %>" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
      <div class="panel-body">
         <ul>
          <% theme.categories.each do |category| %>
            <li><%= link_to category.name, category %></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
  <% end %> 
</div>

</div>



